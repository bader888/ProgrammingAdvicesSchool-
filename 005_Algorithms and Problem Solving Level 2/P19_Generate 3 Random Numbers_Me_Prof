using System;
using System.Collections.Generic;
using System.Linq;

namespace lab
{
    class Program
    {
        static void Main(string[] args)
        {
            printResult();
        }
        static int generateRandomNumberFrom1To10()
        {
            Random rnd = new Random();
            return rnd.Next(1, 10);
        }
        static void printResult()
        {
            for (int i = 0; i < 3; i++)
                Console.WriteLine(generateRandomNumberFrom1To10());
        }

    }
} 	
