using System;
using System.IO;
using System.Linq;
using System.Text;

namespace lab
{
    class Program
    {
        static void Main(string[] args)
        {
            StreamWriter file = new StreamWriter("myFile.txt", append: true);
            file.WriteLine("First Line");
            file.WriteLine("Secound Line");
            file.WriteLine("Third Line");
            file.Close();
        }
    }
}

