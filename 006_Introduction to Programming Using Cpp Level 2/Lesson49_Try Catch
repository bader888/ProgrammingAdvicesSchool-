using System;
using System.Collections.Generic;
using System.Linq;
using System.Runtime.InteropServices;

namespace lab
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] numbers = new int[] { 1, 2, 3, 4, 5 };

            try
            {
                Console.WriteLine(numbers[5]);
            }
            catch
            {
                Console.WriteLine("out of bound");
            }
        }
    }
}

