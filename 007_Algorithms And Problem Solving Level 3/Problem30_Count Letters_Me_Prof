using System;
using System.Collections.Generic;
using System.Linq;

namespace lab
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine(charCountOccurrence("saleem", 'e'));
        }
        static int charCountOccurrence(string s, char c)
        {
            int counter = 0;
            for (int i = 0; i < s.Length; i++)
                if (s[i] == c)
                    counter++;
            return counter;
        }
    }
}
