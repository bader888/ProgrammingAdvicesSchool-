using Lab;

namespace lab
{
    class Program
    {
        static void Main()
        {
            A[] arrA = new A[] { new A(10), new A(20), new A(30) };


            for (int i = 0; i < 3; i++)
                arrA[i].print();
        }
        class A
        {
            int x;
            public A(int value)
            {
                x = value;
            }

            public void print() => Console.WriteLine("The Value of x: {0}", x);
        }
    }
}
